{
    "extends": "airbnb",
    "globals": {
      "fetch": true,
      "navigator": true
    },
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
      "global-require": 0,
      "no-nested-ternary": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prop-types": ["error", { "ignore": ["navigation", "mutate"] }],
      "no-underscore-dangle": ["error", { "allow": ["__typename", "_bar"] }]
    }
}